{
    "java.project.sourcePaths": [
        "Undirbúningur í Java",
        "Töl-101",
        ""
    ],
    "autoHeader": {
        "format": {
            "startWith": "/**",
            "middleWith": "*",
            "endWith": "*/",
            "headerPrefix": "@",
        },
        "header": {
            "Höfundur": "\tÞorvaldur Tumi Baldursson",
            "Netfang": "\tttb3@hi.is",
            "Búið til": {
                "type": "createTime",
                "format": "\tYYYY-DD-MM HH:mm",
            },
            "Lýsing": "\t",
        }
    },
    "files.exclude": {
        "**/*.class": true
    },
    "java.project.referencedLibraries": [
        "lib/**/*.jar",
        "stdlib.jar",
        "libs/stdlib.jar"
    ]
}

